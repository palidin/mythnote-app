    "@replit/codemirror-lang-csharp": "^6.2.0",
    "@szhsin/react-menu": "^3.5.3",
    "ahooks": "^3.7.8",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "rc-dialog": "^9.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-toastify": "^9.1.3",
    "uuid": "^9.0.1",
    "styled-components": "5.3.6",
    "zustand": "4.3.3"
  },
  "devDependencies": {
    "@rsbuild/core": "^0.4.7",
    "@rsbuild/plugin-react": "^0.4.7",
    "@types/node": "^20.11.22",
    "@types/react": "18",
    "@types/react-dom": "18",
    "typescript": "^5.3.3"
  },
  "optimizeDeps": {
    "exclude": [
