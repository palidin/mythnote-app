{
  "name": "mythnote",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "start": "rsbuild dev --open",
    "build": "rsbuild build",
    "preview": "rsbuild preview"
  },
  "dependencies": {
    "@mythnote/markdown-editor": "workspace:*",
    "@mythnote/editable-wysiwyg-editor": "workspace:*",
